<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/18" fx:controller="tictactoe.tictactoe.MainMenuController">
    <padding>
        <Insets bottom="80.0" left="80.0" right="80.0" top="80.0" />
    </padding>
   <Button  onAction="#onStartGameButtonClick" text="Start Game" >
      <font>
         <Font size="14.0" />
      </font>
   </Button>
    <Button onAction="#onExitButtonClick" text="Exit" />
</VBox>
